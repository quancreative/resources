package pdh.events{	import flash.events.Event;		public class ExternalLoaderEvent extends Event 	{		public static const COMPLETE:String = "pv loaded";		public static const LOADED	:String = "pv loaded";		public static const LOADING	:String = "pv loading";		public static const VISIBLE	:String = "pv vis";				public var parameters			:Object;				public function ExternalLoaderEvent(command:String, _param:Object = null) {			super(command, true);					parameters = _param;		}	}}