package pdh.events {	import flash.events.Event;		public class ScrollBarEvent extends Event {		public static const UPDATE:String = "update";		public static const NEXT:String = "next";		public static const PREV:String = "prev";		public static const TWEEN_DONE:String = "tween done";		public static const TWEEN_START:String = "tween start";		public var scroll_value:Number;		public var value:Number;				public function ScrollBarEvent( _type:String, _value:Number = 0 ) {			super( _type );			scroll_value = _value;			value = _value;		}	}}